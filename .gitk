set mainfont {Ubuntu 8}
set textfont {{DejaVu Sans Mono} 9}
set uifont {Ubuntu 9 bold}
set tabstop 8
set findmergefiles 0
set maxgraphpct 50
set maxwidth 16
set cmitmode patch
set wrapcomment none
set autoselect 1
set showneartags 1
set hideremotes 0
set showlocalchanges 1
set datetimeformat {%Y-%m-%d %H:%M:%S}
set limitdiffs 1
set uicolor grey85
set want_ttk 1
set bgcolor white
set fgcolor black
set colors {green red blue magenta darkgrey brown orange}
set diffcolors {red "#00a000" blue}
set markbgcolor #e0e0ff
set diffcontext 3
set selectbgcolor gray85
set extdifftool meld
set perfile_attrs 0
set geometry(main) 1440x846+0+0
set geometry(state) normal
set geometry(topwidth) 1440
set geometry(topheight) 441
set geometry(pwsash0) "552 1"
set geometry(pwsash1) "874 1"
set geometry(botwidth) 529
set geometry(botheight) 400
set permviews {{Everything {} {--all --branches --tags --remotes} {}}
}
